#include <stdio.h>

int main() {
    int num, reversed = 0;

    // Input a number
    printf("Enter an integer: ");
    scanf("%d", &num);

    // Reverse the number
    while (num != 0) {
        int digit = num % 10;
        reversed = reversed * 10 + digit;
        num /= 10;
    }

    // Output the reversed number
    printf("Reversed number: %d\n", reversed);

    return 0;
}
